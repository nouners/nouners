    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.19.24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.1.3",
    "@vitest/ui": "^2.1.3",
    "dotenv": "^16.6.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.5.6",
    "eslint-config-prettier": "^9.1.2",
    "jsdom": "^24.0.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.8.3",
    "vitest": "^2.1.9",
    "wrangler": "4.16.1"
